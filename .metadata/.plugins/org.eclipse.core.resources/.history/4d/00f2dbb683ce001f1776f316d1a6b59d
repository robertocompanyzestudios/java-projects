package programacionPOO;

public class Main {

	public static void main(String[] args) {
		Automovil auto = new Automovil();
		
		auto.cilindrada = 100;
		auto.marca = "Kia";
		auto.modelo = "Carnival";
		
		System.out.println(auto.detalle());
		System.out.println(auto.acelerar(auto.velocidad));
		System.out.println(auto.detalle());
		
		System.out.println("El automovil" + auto.marca + " " + auto.modelo + " ha tardado en frenar"
		+ auto.frenarAuto(120) + "s");
		
	}
	
}
